{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "ml-service/*.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/ml",
      "dest": "/ml-service/predict_ml.py"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
