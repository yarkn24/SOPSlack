{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "*.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.js",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    },
    {
      "src": "*.gif",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.svg",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.png",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/predict",
      "dest": "/api/predict.py"
    },
    {
      "src": "/api/parse",
      "dest": "/api/parse.py"
    },
    {
      "src": "/api/analyze",
      "dest": "/api/analyze.py"
    },
    {
      "src": "/api/sop",
      "dest": "/api/sop.py"
    },
    {
      "src": "/favicon\\.(svg|png|ico)",
      "dest": "/public/favicon.$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
